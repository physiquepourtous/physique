  color: white;
  padding: 10px 20px;
  border-radius: 4px;
  text-decoration: none;
  font-weight: bold;
  margin: 20px 0;
  transition: background-color 0.3s;
}

.subscribe-button:hover {
  background-color: #cc0000;
  text-decoration: none;
  color: white;
}

.subscribe-button i {
  margin-right: 8px;
}
</style>
